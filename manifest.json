{
	"manifest_version": 2,
	"name": "hrscanner",
	"description": "hrscanner",
	"version": "1.0",
	"browser_action": {      
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab"
	],
	
	"icons": { "16": "hsc.png",
				"48": "hsc.png" },

	"content_scripts": [
		{
		"matches": [
			"http://*/*",
			"https://*/*"
		],
		"css": [ "popup.css"],
		"js": ["popup.js"]
		}
	]
}